#################################################################
#    Object Properties
#################################################################

###  http://www.semanticweb.org/paolo/ontologies/2021/11/untitled-ontology-16#appartieneA
:appartieneA rdf:type owl:ObjectProperty ;
             owl:inverseOf :definisce ;
             rdfs:domain :Risorsa_letteraria ;
             rdfs:range :Genere_letterario .


###  http://www.semanticweb.org/paolo/ontologies/2021/11/untitled-ontology-16#contiene
:contiene rdf:type owl:ObjectProperty ;
          owl:inverseOf :ècontenutoDa ;
          rdfs:domain :Collezione ;
          rdfs:range :Risorsa_letteraria .


###  http://www.semanticweb.org/paolo/ontologies/2021/11/untitled-ontology-16#definisce
:definisce rdf:type owl:ObjectProperty ;
           rdfs:domain :Genere_letterario ;
           rdfs:range :Risorsa_letteraria .


###  http://www.semanticweb.org/paolo/ontologies/2021/11/untitled-ontology-16#haIdeato_collezione
:haIdeato_collezione rdf:type owl:ObjectProperty ;
                     rdfs:domain :Autore ;
                     rdfs:range :Collezione ;
                     owl:propertyChainAxiom ( :ha_scritto
                                              :ècontenutoDa
                                            ) .


###  http://www.semanticweb.org/paolo/ontologies/2021/11/untitled-ontology-16#ha_scritto
:ha_scritto rdf:type owl:ObjectProperty ;
            owl:inverseOf :è_stato_scritto ;
            rdfs:domain :Autore ;
            rdfs:range :Risorsa_letteraria .


###  http://www.semanticweb.org/paolo/ontologies/2021/11/untitled-ontology-16#legge
:legge rdf:type owl:ObjectProperty ;
       owl:inverseOf :è_letto ;
       rdfs:domain :Lettore ;
       rdfs:range :Risorsa_letteraria .


###  http://www.semanticweb.org/paolo/ontologies/2021/11/untitled-ontology-16#macro_trattataDa
:macro_trattataDa rdf:type owl:ObjectProperty ;
                  owl:inverseOf :trattaDi_macro ;
                  rdfs:domain :Tematica ;
                  rdfs:range :Risorsa_letteraria ;
                  owl:propertyChainAxiom ( :parlaDi
                                           :è_trattataDa
                                         ) .


###  http://www.semanticweb.org/paolo/ontologies/2021/11/untitled-ontology-16#macro_èCorrelata
:macro_èCorrelata rdf:type owl:ObjectProperty ;
                  owl:inverseOf :èCorrelato_daMacro ;
                  rdfs:domain :Tematica ;
                  rdfs:range :Genere_letterario ;
                  owl:propertyChainAxiom ( :parlaDi
                                           :èCorrelata
                                         ) .


###  http://www.semanticweb.org/paolo/ontologies/2021/11/untitled-ontology-16#parlaDi
:parlaDi rdf:type owl:ObjectProperty ;
         owl:inverseOf :èAffrontataIn ;
         rdf:type owl:TransitiveProperty ;
         rdfs:domain :MacroTematica ;
         rdfs:range :Tematica .


###  http://www.semanticweb.org/paolo/ontologies/2021/11/untitled-ontology-16#studia
:studia rdf:type owl:ObjectProperty ;
        rdfs:subPropertyOf :legge ;
        owl:inverseOf :è_studiatoDa ;
        rdfs:domain :Studente ;
        rdfs:range :Libro_scolastico .


###  http://www.semanticweb.org/paolo/ontologies/2021/11/untitled-ontology-16#trattaDi
:trattaDi rdf:type owl:ObjectProperty ;
          owl:inverseOf :è_trattataDa ;
          rdfs:domain :Risorsa_letteraria ;
          rdfs:range :Tematica .


###  http://www.semanticweb.org/paolo/ontologies/2021/11/untitled-ontology-16#trattaDi_daGenere
:trattaDi_daGenere rdf:type owl:ObjectProperty ;
                   owl:inverseOf :èTrattataDa_daGenere ;
                   rdfs:domain :Risorsa_letteraria ;
                   rdfs:range :Tematica ;
                   owl:propertyChainAxiom ( :appartieneA
                                            :èCorrelato
                                          ) ,
                                          ( :appartieneA
                                            :èCorrelato_daMacro
                                          ) .


###  http://www.semanticweb.org/paolo/ontologies/2021/11/untitled-ontology-16#trattaDi_macro
:trattaDi_macro rdf:type owl:ObjectProperty ;
                rdfs:domain :Risorsa_letteraria ;
                rdfs:range :Tematica ;
                owl:propertyChainAxiom ( :trattaDi
                                         :èAffrontataIn
                                       ) .


###  http://www.semanticweb.org/paolo/ontologies/2021/11/untitled-ontology-16#èAffrontataIn
:èAffrontataIn rdf:type owl:ObjectProperty ,
                        owl:TransitiveProperty ;
               rdfs:domain :Tematica ;
               rdfs:range :MacroTematica .


###  http://www.semanticweb.org/paolo/ontologies/2021/11/untitled-ontology-16#èCorrelata
:èCorrelata rdf:type owl:ObjectProperty ;
            owl:inverseOf :èCorrelato ;
            rdfs:domain :Tematica ;
            rdfs:range :Genere_letterario .


###  http://www.semanticweb.org/paolo/ontologies/2021/11/untitled-ontology-16#èCorrelato
:èCorrelato rdf:type owl:ObjectProperty ;
            rdfs:domain :Genere_letterario ;
            rdfs:range :Tematica .


###  http://www.semanticweb.org/paolo/ontologies/2021/11/untitled-ontology-16#èCorrelato_daMacro
:èCorrelato_daMacro rdf:type owl:ObjectProperty ;
                    rdfs:domain :Genere_letterario ;
                    rdfs:range :Tematica ;
                    owl:propertyChainAxiom ( :èCorrelato
                                             :parlaDi
                                           ) .


###  http://www.semanticweb.org/paolo/ontologies/2021/11/untitled-ontology-16#èTrattataDa_daGenere
:èTrattataDa_daGenere rdf:type owl:ObjectProperty ;
                      rdfs:domain :Tematica ;
                      rdfs:range :Risorsa_letteraria ;
                      owl:propertyChainAxiom ( :macro_èCorrelata
                                               :definisce
                                             ) ,
                                             ( :èCorrelata
                                               :definisce
                                             ) .


###  http://www.semanticweb.org/paolo/ontologies/2021/11/untitled-ontology-16#è_letto
:è_letto rdf:type owl:ObjectProperty ;
         rdfs:domain :Risorsa_letteraria ;
         rdfs:range :Lettore .


###  http://www.semanticweb.org/paolo/ontologies/2021/11/untitled-ontology-16#è_stato_scritto
:è_stato_scritto rdf:type owl:ObjectProperty ;
                 rdfs:domain :Risorsa_letteraria ;
                 rdfs:range :Autore .


###  http://www.semanticweb.org/paolo/ontologies/2021/11/untitled-ontology-16#è_studiatoDa
:è_studiatoDa rdf:type owl:ObjectProperty ;
              rdfs:subPropertyOf :è_letto ;
              rdfs:domain :Libro_scolastico ;
              rdfs:range :Studente .


###  http://www.semanticweb.org/paolo/ontologies/2021/11/untitled-ontology-16#è_trattataDa
:è_trattataDa rdf:type owl:ObjectProperty ;
              rdfs:domain :Tematica ;
              rdfs:range :Risorsa_letteraria .


###  http://www.semanticweb.org/paolo/ontologies/2021/11/untitled-ontology-16#ècontenutoDa
:ècontenutoDa rdf:type owl:ObjectProperty ;
              rdfs:domain :Risorsa_letteraria ;
              rdfs:range :Collezione .


#################################################################
#    Data properties
#################################################################

###  http://www.semanticweb.org/paolo/ontologies/2021/11/untitled-ontology-16#codice_lettura
:codice_lettura rdf:type owl:DatatypeProperty ,
                         owl:FunctionalProperty ;
                rdfs:domain :Fumetto ;
                rdfs:range [ rdf:type rdfs:Datatype ;
                             owl:oneOf [ rdf:type rdf:List ;
                                         rdf:first "dx_sx" ;
                                         rdf:rest [ rdf:type rdf:List ;
                                                    rdf:first "sx_dx" ;
                                                    rdf:rest rdf:nil
                                                  ]
                                       ]
                           ] .


###  http://www.semanticweb.org/paolo/ontologies/2021/11/untitled-ontology-16#compostoDa_vignette
:compostoDa_vignette rdf:type owl:DatatypeProperty ;
                     rdfs:domain :Risorsa_letteraria ;
                     rdfs:range xsd:boolean .


###  http://www.semanticweb.org/paolo/ontologies/2021/11/untitled-ontology-16#contiene_istruzioni
:contiene_istruzioni rdf:type owl:DatatypeProperty ,
                              owl:FunctionalProperty ;
                     rdfs:domain :Risorsa_letteraria ;
                     rdfs:range xsd:boolean .


###  http://www.semanticweb.org/paolo/ontologies/2021/11/untitled-ontology-16#descrive_lingua
:descrive_lingua rdf:type owl:DatatypeProperty ;
                 rdfs:subPropertyOf :èIn_lingua ;
                 rdfs:domain :Dizionario ;
                 rdfs:range xsd:language .


###  http://www.semanticweb.org/paolo/ontologies/2021/11/untitled-ontology-16#haDataNascita
:haDataNascita rdf:type owl:DatatypeProperty ,
                        owl:FunctionalProperty ;
               rdfs:domain <http://xmlns.com/foaf/0.1/Person> ;
               rdfs:range xsd:dateTime .


###  http://www.semanticweb.org/paolo/ontologies/2021/11/untitled-ontology-16#haPunteggio
:haPunteggio rdf:type owl:DatatypeProperty ,
                      owl:FunctionalProperty ;
             rdfs:domain :Risorsa_letteraria ;
             rdfs:range xsd:float .


###  http://www.semanticweb.org/paolo/ontologies/2021/11/untitled-ontology-16#haRiferimento
:haRiferimento rdf:type owl:DatatypeProperty ,
                        owl:FunctionalProperty ;
               rdfs:domain :Risorsa_letteraria ;
               rdfs:range xsd:string .


###  http://www.semanticweb.org/paolo/ontologies/2021/11/untitled-ontology-16#haVersione
:haVersione rdf:type owl:DatatypeProperty ;
            rdfs:domain :Risorsa_letteraria ;
            rdfs:range [ rdf:type rdfs:Datatype ;
                         owl:oneOf [ rdf:type rdf:List ;
                                     rdf:first "Digitale" ;
                                     rdf:rest [ rdf:type rdf:List ;
                                                rdf:first "Fisico" ;
                                                rdf:rest rdf:nil
                                              ]
                                   ]
                       ] .


###  http://www.semanticweb.org/paolo/ontologies/2021/11/untitled-ontology-16#ha_nazionalità
:ha_nazionalità rdf:type owl:DatatypeProperty ,
                         owl:FunctionalProperty ;
                rdfs:domain <http://xmlns.com/foaf/0.1/Person> ;
                rdfs:range xsd:string .


###  http://www.semanticweb.org/paolo/ontologies/2021/11/untitled-ontology-16#ha_nome
:ha_nome rdf:type owl:DatatypeProperty ,
                  owl:FunctionalProperty ;
         rdfs:domain <http://xmlns.com/foaf/0.1/Person> ;
         rdfs:range xsd:string .


###  http://www.semanticweb.org/paolo/ontologies/2021/11/untitled-ontology-16#ha_numpag
:ha_numpag rdf:type owl:DatatypeProperty ,
                    owl:FunctionalProperty ;
           rdfs:domain :Risorsa_letteraria ;
           rdfs:range xsd:nonNegativeInteger .


###  http://www.semanticweb.org/paolo/ontologies/2021/11/untitled-ontology-16#ha_periodicità
:ha_periodicità rdf:type owl:DatatypeProperty ;
                rdfs:domain :Rivista ;
                rdfs:range [ rdf:type rdfs:Datatype ;
                             owl:oneOf [ rdf:type rdf:List ;
                                         rdf:first "annuale" ;
                                         rdf:rest [ rdf:type rdf:List ;
                                                    rdf:first "mensile" ;
                                                    rdf:rest [ rdf:type rdf:List ;
                                                               rdf:first "settimanale" ;
                                                               rdf:rest rdf:nil
                                                             ]
                                                  ]
                                       ]
                           ] .


###  http://www.semanticweb.org/paolo/ontologies/2021/11/untitled-ontology-16#ha_titolo
:ha_titolo rdf:type owl:DatatypeProperty ,
                    owl:FunctionalProperty ;
           rdfs:domain :Risorsa_letteraria ;
           rdfs:range xsd:string .


###  http://www.semanticweb.org/paolo/ontologies/2021/11/untitled-ontology-16#ha_trama
:ha_trama rdf:type owl:DatatypeProperty ,
                   owl:FunctionalProperty ;
          rdfs:domain :Risorsa_letteraria ;
          rdfs:range xsd:string .


###  http://www.semanticweb.org/paolo/ontologies/2021/11/untitled-ontology-16#tipo_versi
:tipo_versi rdf:type owl:DatatypeProperty ,
                     owl:FunctionalProperty ;
            rdfs:domain :Poesia ;
            rdfs:range [ rdf:type rdfs:Datatype ;
                         owl:oneOf [ rdf:type rdf:List ;
                                     rdf:first "alternata" ;
                                     rdf:rest [ rdf:type rdf:List ;
                                                rdf:first "baciata" ;
                                                rdf:rest [ rdf:type rdf:List ;
                                                           rdf:first "incatenata" ;
                                                           rdf:rest [ rdf:type rdf:List ;
                                                                      rdf:first "incrociata" ;
                                                                      rdf:rest [ rdf:type rdf:List ;
                                                                                 rdf:first "sciolta" ;
                                                                                 rdf:rest rdf:nil
                                                                               ]
                                                                    ]
                                                         ]
                                              ]
                                   ]
                       ] .


###  http://www.semanticweb.org/paolo/ontologies/2021/11/untitled-ontology-16#èIn_lingua
:èIn_lingua rdf:type owl:DatatypeProperty ;
            rdfs:domain :Risorsa_letteraria ;
            rdfs:range xsd:language .


###  http://www.semanticweb.org/paolo/ontologies/2021/11/untitled-ontology-16#èRaccoltaOrdinata
:èRaccoltaOrdinata rdf:type owl:DatatypeProperty ,
                            owl:FunctionalProperty ;
                   rdfs:domain :Risorsa_letteraria ;
                   rdfs:range xsd:boolean .


###  http://www.semanticweb.org/paolo/ontologies/2021/11/untitled-ontology-16#èuscitoIn
:èuscitoIn rdf:type owl:DatatypeProperty ,
                    owl:FunctionalProperty ;
           rdfs:domain :Risorsa_letteraria ;
           rdfs:range xsd:dateTimeStamp .


#################################################################
#    Classes
#################################################################

###  http://www.semanticweb.org/paolo/ontologies/2021/11/untitled-ontology-16#Autore
:Autore rdf:type owl:Class ;
        owl:equivalentClass [ rdf:type owl:Restriction ;
                              owl:onProperty :ha_scritto ;
                              owl:someValuesFrom :Risorsa_letteraria
                            ] ;
        rdfs:subClassOf <http://xmlns.com/foaf/0.1/Person> ;
        rdf:type "skos:Concept" ;
        rdfs:comment "Persona maggiorenne che scrive libri"@it ;
        rdfs:label "Autore"@it ;
        <http://www.w3.org/2004/02/skos/core#broadMatch> <http://purl.org/dc/elements/1.1/creator> .


###  http://www.semanticweb.org/paolo/ontologies/2021/11/untitled-ontology-16#Collezione
:Collezione rdf:type owl:Class ;
            owl:equivalentClass [ rdf:type owl:Restriction ;
                                  owl:onProperty :contiene ;
                                  owl:minQualifiedCardinality "2"^^xsd:nonNegativeInteger ;
                                  owl:onClass :Risorsa_letteraria
                                ] ;
            rdf:type "skos:Concept" ;
            <http://www.w3.org/2004/02/skos/core#broadMatch> <http://purl.org/ontology/bibo/CollectedDocument> ,
                                                             "http://purl.org/dc/dcmitype/Collection" ,
                                                             "http://purl.org/ontology/bibo/Collection" ,
                                                             "http://purl.org/ontology/bibo/Series" ,
                                                             "http://purl.org/spar/fabio/Catalog" ;
            <http://www.w3.org/2004/02/skos/core#narrowMatch> "http://purl.org/ontology/bibo/Book" ,
                                                              "http://purl.org/ontology/bibo/Document" .


###  http://www.semanticweb.org/paolo/ontologies/2021/11/untitled-ontology-16#Dizionario
:Dizionario rdf:type owl:Class ;
            owl:equivalentClass [ owl:intersectionOf ( [ rdf:type owl:Restriction ;
                                                         owl:onProperty :descrive_lingua ;
                                                         owl:someValuesFrom xsd:language
                                                       ]
                                                       [ rdf:type owl:Restriction ;
                                                         owl:onProperty :èRaccoltaOrdinata ;
                                                         owl:someValuesFrom [ rdf:type rdfs:Datatype ;
                                                                              owl:oneOf [ rdf:type rdf:List ;
                                                                                          rdf:first "true"^^xsd:boolean ;
                                                                                          rdf:rest rdf:nil
                                                                                        ]
                                                                            ]
                                                       ]
                                                     ) ;
                                  rdf:type owl:Class
                                ] ;
            rdfs:subClassOf :Libro_Classico ;
            rdfs:label "Dizionario"@it ;
            <http://www.w3.org/2004/02/skos/core#exactMatch> "http://purl.org/spar/fabio/TermDictionary" ,
                                                             "http://purl.org/spar/fabio/Vocabulary" .


###  http://www.semanticweb.org/paolo/ontologies/2021/11/untitled-ontology-16#Enciclopedia
:Enciclopedia rdf:type owl:Class ;
              owl:equivalentClass [ owl:intersectionOf ( [ rdf:type owl:Class ;
                                                           owl:unionOf ( [ rdf:type owl:Restriction ;
                                                                           owl:onProperty :appartieneA ;
                                                                           owl:someValuesFrom [ rdf:type owl:Class ;
                                                                                                owl:oneOf ( :genere_artistico
                                                                                                          )
                                                                                              ]
                                                                         ]
                                                                         [ rdf:type owl:Restriction ;
                                                                           owl:onProperty :appartieneA ;
                                                                           owl:someValuesFrom [ rdf:type owl:Class ;
                                                                                                owl:oneOf ( :genere_scientifico
                                                                                                          )
                                                                                              ]
                                                                         ]
                                                                       )
                                                         ]
                                                         [ rdf:type owl:Restriction ;
                                                           owl:onProperty :èRaccoltaOrdinata ;
                                                           owl:someValuesFrom [ rdf:type rdfs:Datatype ;
                                                                                owl:oneOf [ rdf:type rdf:List ;
                                                                                            rdf:first "true"^^xsd:boolean ;
                                                                                            rdf:rest rdf:nil
                                                                                          ]
                                                                              ]
                                                         ]
                                                       ) ;
                                    rdf:type owl:Class
                                  ] ;
              rdfs:subClassOf :Libro_Classico ;
              rdfs:label "Enciclopedia"@it .


###  http://www.semanticweb.org/paolo/ontologies/2021/11/untitled-ontology-16#Fumettista
:Fumettista rdf:type owl:Class ;
            owl:equivalentClass [ rdf:type owl:Restriction ;
                                  owl:onProperty :ha_scritto ;
                                  owl:someValuesFrom :Fumetto
                                ] ;
            rdfs:subClassOf :Autore ;
            rdfs:comment "Autore che scrive fumetti"@it ;
            rdfs:label "Fumettista"@it .


###  http://www.semanticweb.org/paolo/ontologies/2021/11/untitled-ontology-16#Fumetto
:Fumetto rdf:type owl:Class ;
         owl:equivalentClass [ rdf:type owl:Restriction ;
                               owl:onProperty :compostoDa_vignette ;
                               owl:hasValue "true"^^xsd:boolean
                             ] ;
         rdfs:subClassOf :Risorsa_letteraria ;
         owl:disjointUnionOf ( :Fumetto_Occidentale
                               :Manga
                             ) ;
         rdfs:label "Fumetto"@it ;
         <http://www.w3.org/2004/02/skos/core#narrowMatch> "http://purl.org/eprint/type/BookItem" ,
                                                           "http://purl.org/ontology/bibo/Chapter" ,
                                                           "http://purl.org/spar/fabio/ArtisticWork" ,
                                                           "http://purl.org/spar/fabio/Image" .


###  http://www.semanticweb.org/paolo/ontologies/2021/11/untitled-ontology-16#Fumetto_Occidentale
:Fumetto_Occidentale rdf:type owl:Class ;
                     owl:equivalentClass [ rdf:type owl:Restriction ;
                                           owl:onProperty :codice_lettura ;
                                           owl:hasValue "sx_dx"
                                         ] ;
                     rdfs:subClassOf :Fumetto ;
                     rdfs:label "Fumetto_Occidentale"@it .


###  http://www.semanticweb.org/paolo/ontologies/2021/11/untitled-ontology-16#Genere_letterario
:Genere_letterario rdf:type owl:Class ;
                   owl:equivalentClass [ rdf:type owl:Class ;
                                         owl:oneOf ( :genere_artistico
