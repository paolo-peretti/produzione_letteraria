@prefix : <http://www.semanticweb.org/produzione_letteraria#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix foaf: <http://xmlns.com/foaf/0.1/> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix vann: <http://purl.org/vocab/vann/> .
@prefix produzione_letteraria: <http://www.semanticweb.org/produzione_letteraria#> .
@base <http://www.semanticweb.org/produzione_letteraria> .

<http://www.semanticweb.org/produzione_letteraria> rdf:type owl:Ontology ;
                                                    dc:creator "Alberto Remondino" ,
                                                               "Paolo Peretti" ;
                                                    dc:date "2022-01-17T11:00:00.00" ;
                                                    dc:description """L'ontologia ha lo scopo di rappresentare la conoscenza relativa alla produzione letteraria. 
Vengono considerati i possibili collegamenti con varie classi anche per poter comprendere in che modo una risorsa letteraria possa essere accomunata ad un altra. 
Infatti l' ontologia può essere anche usata per consigliare ad un utente nuove letture in base ad i suoi interessi."""@it ;
                                                    dc:title "Ontologia sulla produzione letteraria"@it ;
                                                    vann:preferredNamespacePrefix "pl"@it ;
                                                    vann:preferredNamespaceUri <http://www.semanticweb.org/produzione_letteraria> ;
                                                    rdfs:label "produzione_letteraria"@it .

#################################################################
#    Annotation properties
#################################################################

