skos:exactMatch rdf:type owl:AnnotationProperty .


###  http://www.w3.org/2004/02/skos/core#narrowMatch
skos:narrowMatch rdf:type owl:AnnotationProperty .


###  http://www.w3.org/2004/02/skos/core#prefLabel
skos:prefLabel rdf:type owl:AnnotationProperty .


###  http://xmlns.com/foaf/0.1/Person
foaf:Person rdf:type owl:AnnotationProperty .


#################################################################
#    Object Properties
#################################################################

###  http://www.semanticweb.org/produzione_letteraria#appartieneA
produzione_letteraria:appartieneA rdf:type owl:ObjectProperty ;
                                  owl:inverseOf produzione_letteraria:definisce ;
                                  rdfs:domain produzione_letteraria:Risorsa_letteraria ;
                                  rdfs:range produzione_letteraria:Genere_letterario ;
                                  rdfs:comment "Una risorsa letteraria appartiene ad un genere letterario"@it ;
                                  rdfs:label "appartieneA"@it .


###  http://www.semanticweb.org/produzione_letteraria#contiene
produzione_letteraria:contiene rdf:type owl:ObjectProperty ;
                               owl:inverseOf produzione_letteraria:ècontenutoDa ;
                               rdfs:domain produzione_letteraria:Collezione ;
                               rdfs:range produzione_letteraria:Risorsa_letteraria ;
                               rdfs:comment "Una collezione contiene una risorsa letteraria"@it ;
                               rdfs:label "contiene"@it .


###  http://www.semanticweb.org/produzione_letteraria#definisce
produzione_letteraria:definisce rdf:type owl:ObjectProperty ;
                                rdfs:domain produzione_letteraria:Genere_letterario ;
                                rdfs:range produzione_letteraria:Risorsa_letteraria ;
                                rdfs:comment "Un genere letterario definisce una risorsa letteraria"@it ;
                                rdfs:label "definisce"@it .


###  http://www.semanticweb.org/produzione_letteraria#haIdeato_collezione
produzione_letteraria:haIdeato_collezione rdf:type owl:ObjectProperty ;
                                          rdfs:domain produzione_letteraria:Autore ;
                                          rdfs:range produzione_letteraria:Collezione ;
                                          owl:propertyChainAxiom ( produzione_letteraria:ha_scritto
                                                                   produzione_letteraria:ècontenutoDa
                                                                 ) ;
                                          rdfs:comment "Un autore ha ideato una collezione se ha scritto una risorsa che fa parte di quella collezione"@it ;
                                          rdfs:label "haIdeato_collezione"@it .


###  http://www.semanticweb.org/produzione_letteraria#ha_scritto
produzione_letteraria:ha_scritto rdf:type owl:ObjectProperty ;
                                 owl:inverseOf produzione_letteraria:è_stato_scritto ;
                                 rdfs:domain produzione_letteraria:Autore ;
                                 rdfs:range produzione_letteraria:Risorsa_letteraria ;
                                 rdfs:comment "Un autore ha scritto (ha prodotto) una risorsa letteraria"@it ;
                                 rdfs:label "ha_scritto"@it .


###  http://www.semanticweb.org/produzione_letteraria#legge
produzione_letteraria:legge rdf:type owl:ObjectProperty ;
                            owl:inverseOf produzione_letteraria:è_letto ;
                            rdfs:domain produzione_letteraria:Lettore ;
                            rdfs:range produzione_letteraria:Risorsa_letteraria ;
                            rdfs:comment "Il lettore legge una risorsa letteraria"@it ;
                            rdfs:label "legge"@it .


###  http://www.semanticweb.org/produzione_letteraria#macro_trattataDa
produzione_letteraria:macro_trattataDa rdf:type owl:ObjectProperty ;
                                       owl:inverseOf produzione_letteraria:trattaDi_macro ;
                                       rdfs:domain produzione_letteraria:Tematica ;
                                       rdfs:range produzione_letteraria:Risorsa_letteraria ;
                                       owl:propertyChainAxiom ( produzione_letteraria:parlaDi
                                                                produzione_letteraria:è_trattataDa
                                                              ) ;
                                       rdfs:comment "Una macro tematica parla di un'altro argomento, se quell'argomento è trattato da una risorsa letteraria, la risorsa a sua volta tratterà la macro tematica associata all'argomento"@it ;
                                       rdfs:label "macro_trattataDa"@it .


###  http://www.semanticweb.org/produzione_letteraria#macro_èCorrelata
produzione_letteraria:macro_èCorrelata rdf:type owl:ObjectProperty ;
                                       owl:inverseOf produzione_letteraria:èCorrelato_daMacro ;
                                       rdfs:domain produzione_letteraria:Tematica ;
                                       rdfs:range produzione_letteraria:Genere_letterario ;
                                       owl:propertyChainAxiom ( produzione_letteraria:parlaDi
                                                                produzione_letteraria:èCorrelata
                                                              ) ;
                                       rdfs:comment "Una macro tematica parla di un'altro argomento che è correlato con un genere letterario, quindi quel genere letterario sarà a sua volta correlato con la macro tematica"@it ;
                                       rdfs:label "macro_èCorrelata"@it .


###  http://www.semanticweb.org/produzione_letteraria#parlaDi
produzione_letteraria:parlaDi rdf:type owl:ObjectProperty ;
                              owl:inverseOf produzione_letteraria:èAffrontataIn ;
                              rdf:type owl:TransitiveProperty ;
                              rdfs:domain produzione_letteraria:MacroTematica ;
                              rdfs:range produzione_letteraria:Tematica ;
                              rdfs:comment "Una macro tematica parla di una tematica più specifica"@it ;
                              rdfs:label "parlaDi"@it .


###  http://www.semanticweb.org/produzione_letteraria#studia
produzione_letteraria:studia rdf:type owl:ObjectProperty ;
                             rdfs:subPropertyOf produzione_letteraria:legge ;
                             owl:inverseOf produzione_letteraria:è_studiatoDa ;
                             rdfs:domain produzione_letteraria:Studente ;
                             rdfs:range produzione_letteraria:Libro_scolastico ;
                             rdfs:comment "Una persona studia un libro scolastico, quindi è uno studente"@it ;
                             rdfs:label "studia"@it .


###  http://www.semanticweb.org/produzione_letteraria#trattaDi
produzione_letteraria:trattaDi rdf:type owl:ObjectProperty ;
                               owl:inverseOf produzione_letteraria:è_trattataDa ;
                               rdfs:domain produzione_letteraria:Risorsa_letteraria ;
                               rdfs:range produzione_letteraria:Tematica ;
                               rdfs:comment "Una risorsa letteraria tratta di una tematica"@it ;
                               rdfs:label "trattaDi"@it .


###  http://www.semanticweb.org/produzione_letteraria#trattaDi_daGenere
produzione_letteraria:trattaDi_daGenere rdf:type owl:ObjectProperty ;
                                        owl:inverseOf produzione_letteraria:èTrattataDa_daGenere ;
                                        rdfs:domain produzione_letteraria:Risorsa_letteraria ;
                                        rdfs:range produzione_letteraria:Tematica ;
                                        owl:propertyChainAxiom ( produzione_letteraria:appartieneA
                                                                 produzione_letteraria:èCorrelato
                                                               ) ,
                                                               ( produzione_letteraria:appartieneA
                                                                 produzione_letteraria:èCorrelato_daMacro
                                                               ) ;
                                        rdfs:comment "Una risorsa letteraria appartiene ad un genere letterario che è correlato ad una tematica o ad una macro tematica, quindi questa risorsa tratta di questo argomento"@it ;
                                        rdfs:label "trattaDi_daGenere"@it .


###  http://www.semanticweb.org/produzione_letteraria#trattaDi_macro
produzione_letteraria:trattaDi_macro rdf:type owl:ObjectProperty ;
                                     rdfs:domain produzione_letteraria:Risorsa_letteraria ;
                                     rdfs:range produzione_letteraria:Tematica ;
                                     owl:propertyChainAxiom ( produzione_letteraria:trattaDi
                                                              produzione_letteraria:èAffrontataIn
                                                            ) ;
                                     rdfs:comment "Una risorsa letteraria tratta di una tematica che è affrontata da una macro tematica, quindi questa risorsa tratta anche della macro tematica"@it ;
                                     rdfs:label "trattaDi_macro"@it .


###  http://www.semanticweb.org/produzione_letteraria#èAffrontataIn
produzione_letteraria:èAffrontataIn rdf:type owl:ObjectProperty ,
                                             owl:TransitiveProperty ;
                                    rdfs:domain produzione_letteraria:Tematica ;
                                    rdfs:range produzione_letteraria:MacroTematica ;
                                    rdfs:comment "Una tematica viene affrontata in una macro tematica più generale"@it ;
                                    rdfs:label "èAffrontataIn"@it .


###  http://www.semanticweb.org/produzione_letteraria#èCorrelata
produzione_letteraria:èCorrelata rdf:type owl:ObjectProperty ;
                                 owl:inverseOf produzione_letteraria:èCorrelato ;
                                 rdfs:domain produzione_letteraria:Tematica ;
                                 rdfs:range produzione_letteraria:Genere_letterario ;
                                 rdfs:comment "Una tematica è correlata ad un genere letterario"@it ;
                                 rdfs:label "èCorrelata"@it .


###  http://www.semanticweb.org/produzione_letteraria#èCorrelato
produzione_letteraria:èCorrelato rdf:type owl:ObjectProperty ;
                                 rdfs:domain produzione_letteraria:Genere_letterario ;
                                 rdfs:range produzione_letteraria:Tematica ;
                                 rdfs:comment "Un genere letterario è correlato ad una tematica"@it ;
                                 rdfs:label "èCorrelato"@it .


###  http://www.semanticweb.org/produzione_letteraria#èCorrelato_daMacro
produzione_letteraria:èCorrelato_daMacro rdf:type owl:ObjectProperty ;
                                         rdfs:domain produzione_letteraria:Genere_letterario ;
                                         rdfs:range produzione_letteraria:Tematica ;
                                         owl:propertyChainAxiom ( produzione_letteraria:èCorrelato
                                                                  produzione_letteraria:parlaDi
                                                                ) ;
                                         rdfs:comment "Un genere letterario è correlato ad una tematica che parla di un'altra tematica e quindi è correlato a questo argomento più specifico"@it ;
                                         rdfs:label "èCorrelato_daMacro"@it .


###  http://www.semanticweb.org/produzione_letteraria#èTrattataDa_daGenere
produzione_letteraria:èTrattataDa_daGenere rdf:type owl:ObjectProperty ;
                                           rdfs:domain produzione_letteraria:Tematica ;
                                           rdfs:range produzione_letteraria:Risorsa_letteraria ;
                                           owl:propertyChainAxiom ( produzione_letteraria:macro_èCorrelata
                                                                    produzione_letteraria:definisce
                                                                  ) ,
                                                                  ( produzione_letteraria:èCorrelata
                                                                    produzione_letteraria:definisce
                                                                  ) ;
                                           rdfs:comment "Una tematica o macro tematica è correlata ad un genere letterario che definisce una risorsa letteraria, quindi questa tematica è trattata da questa risorsa"@it ;
                                           rdfs:label "èTrattataDa_daGenere"@it .


###  http://www.semanticweb.org/produzione_letteraria#è_letto
produzione_letteraria:è_letto rdf:type owl:ObjectProperty ;
                              rdfs:domain produzione_letteraria:Risorsa_letteraria ;
                              rdfs:range produzione_letteraria:Lettore ;
                              rdfs:comment "Una risorsa letteraria viene letta da un lettore"@it ;
                              rdfs:label "è_letto"@it .


###  http://www.semanticweb.org/produzione_letteraria#è_stato_scritto
produzione_letteraria:è_stato_scritto rdf:type owl:ObjectProperty ;
                                      rdfs:domain produzione_letteraria:Risorsa_letteraria ;
                                      rdfs:range produzione_letteraria:Autore ;
                                      rdfs:comment "Una risorsa è stata scritta da un autore"@it ;
                                      rdfs:label "è_stato_scritto"@it .


###  http://www.semanticweb.org/produzione_letteraria#è_studiatoDa
produzione_letteraria:è_studiatoDa rdf:type owl:ObjectProperty ;
                                   rdfs:subPropertyOf produzione_letteraria:è_letto ;
                                   rdfs:domain produzione_letteraria:Libro_scolastico ;
                                   rdfs:range produzione_letteraria:Studente ;
                                   rdfs:comment "Un libro scolastico viene studiato da uno studente"@it ;
                                   rdfs:label "è_studiatoDa"@it .


###  http://www.semanticweb.org/produzione_letteraria#è_trattataDa
produzione_letteraria:è_trattataDa rdf:type owl:ObjectProperty ;
                                   rdfs:domain produzione_letteraria:Tematica ;
                                   rdfs:range produzione_letteraria:Risorsa_letteraria ;
                                   rdfs:comment "Una tematica viene trattata in una risorsa letteraria"@it ;
                                   rdfs:label "è_trattataDa"@it .


###  http://www.semanticweb.org/produzione_letteraria#ècontenutoDa
produzione_letteraria:ècontenutoDa rdf:type owl:ObjectProperty ;
                                   rdfs:domain produzione_letteraria:Risorsa_letteraria ;
                                   rdfs:range produzione_letteraria:Collezione ;
                                   rdfs:comment "Una  risorsa letteraria è contenuta all'interno di una collezione"@it ;
                                   rdfs:label "ècontenutoDa"@it .


#################################################################
#    Data properties
#################################################################

###  http://www.semanticweb.org/produzione_letteraria#codice_lettura
produzione_letteraria:codice_lettura rdf:type owl:DatatypeProperty ,
                                              owl:FunctionalProperty ;
                                     rdfs:domain produzione_letteraria:Fumetto ;
                                     rdfs:range [ rdf:type rdfs:Datatype ;
                                                  owl:oneOf [ rdf:type rdf:List ;
                                                              rdf:first "dx_sx" ;
                                                              rdf:rest [ rdf:type rdf:List ;
                                                                         rdf:first "sx_dx" ;
                                                                         rdf:rest rdf:nil
                                                                       ]
                                                            ]
                                                ] ;
                                     rdfs:comment "Un fumetto può essere letto da sinistra verso destra o viceversa"@it ;
                                     rdfs:label "codice_lettura"@it .


###  http://www.semanticweb.org/produzione_letteraria#compostoDa_vignette
produzione_letteraria:compostoDa_vignette rdf:type owl:DatatypeProperty ,
                                                   owl:FunctionalProperty ;
                                          rdfs:domain produzione_letteraria:Risorsa_letteraria ;
                                          rdfs:range xsd:boolean ;
                                          rdfs:comment "Una risorsa letteraria può essere composta da vignette (immagini con dialoghi)"@it ;
                                          rdfs:label "compostoDa_vignette"@it .


###  http://www.semanticweb.org/produzione_letteraria#contiene_istruzioni
produzione_letteraria:contiene_istruzioni rdf:type owl:DatatypeProperty ,
                                                   owl:FunctionalProperty ;
                                          rdfs:domain produzione_letteraria:Risorsa_letteraria ;
                                          rdfs:range xsd:boolean ;
                                          rdfs:comment "Una risorsa letteraria può contenere istruzioni, in questo caso si può parlare di un manuale"@it ;
                                          rdfs:label "contiene_istruzioni"@it .


###  http://www.semanticweb.org/produzione_letteraria#descrive_lingua
produzione_letteraria:descrive_lingua rdf:type owl:DatatypeProperty ;
                                      rdfs:subPropertyOf produzione_letteraria:èIn_lingua ;
                                      rdfs:domain produzione_letteraria:Dizionario ;
                                      rdfs:range xsd:language ;
                                      rdfs:comment "Un dizionario è stato scritto in una lingua per descriverla"@it ;
                                      rdfs:label "descrive_lingua"@it .


###  http://www.semanticweb.org/produzione_letteraria#haDataNascita
produzione_letteraria:haDataNascita rdf:type owl:DatatypeProperty ,
                                             owl:FunctionalProperty ;
                                    rdfs:domain foaf:Person ;
                                    rdfs:range xsd:dateTime ;
                                    rdfs:comment "Una persona è nata in una certa data"@it ;
                                    rdfs:label "haDataNascita"@it .


###  http://www.semanticweb.org/produzione_letteraria#haPunteggio
produzione_letteraria:haPunteggio rdf:type owl:DatatypeProperty ,
                                           owl:FunctionalProperty ;
                                  rdfs:domain produzione_letteraria:Risorsa_letteraria ;
                                  rdfs:range [ rdf:type rdfs:Datatype ;
                                               owl:onDatatype xsd:float ;
                                               owl:withRestrictions ( [ xsd:minInclusive "0.0"^^xsd:float
                                                                      ]
                                                                      [ xsd:maxInclusive "10.0"^^xsd:float
                                                                      ]
                                                                    )
                                             ] ;
                                  rdfs:comment "Una risorsa può essere valutata con un punteggio (da 0 a 10)"@it ;
                                  rdfs:label "haPunteggio"@it .


###  http://www.semanticweb.org/produzione_letteraria#haVersione
produzione_letteraria:haVersione rdf:type owl:DatatypeProperty ;
                                 rdfs:domain produzione_letteraria:Risorsa_letteraria ;
                                 rdfs:range [ rdf:type rdfs:Datatype ;
                                              owl:oneOf [ rdf:type rdf:List ;
                                                          rdf:first "Digitale" ;
                                                          rdf:rest [ rdf:type rdf:List ;
                                                                     rdf:first "Fisico" ;
                                                                     rdf:rest rdf:nil
                                                                   ]
                                                        ]
                                            ] ;
                                 rdfs:comment "Una risorsa letteraria viene rilasciata in formato digitale oppure fisico"@it ;
                                 rdfs:label "haVersione"@it .


###  http://www.semanticweb.org/produzione_letteraria#ha_nazionalità
produzione_letteraria:ha_nazionalità rdf:type owl:DatatypeProperty ,
                                              owl:FunctionalProperty ;
                                     rdfs:domain foaf:Person ;
                                     rdfs:range xsd:string ;
                                     rdfs:comment "Una persona ha una determinata nazionalità"@it ;
                                     rdfs:label "ha_nazionalità"@it .


###  http://www.semanticweb.org/produzione_letteraria#ha_nome
produzione_letteraria:ha_nome rdf:type owl:DatatypeProperty ,
                                       owl:FunctionalProperty ;
                              rdfs:domain foaf:Person ;
                              rdfs:range xsd:string ;
                              rdfs:comment "Una persona ha un nome associato"@it ;
                              rdfs:label "ha_nome"@it .


###  http://www.semanticweb.org/produzione_letteraria#ha_numpag
produzione_letteraria:ha_numpag rdf:type owl:DatatypeProperty ,
                                         owl:FunctionalProperty ;
                                rdfs:domain produzione_letteraria:Risorsa_letteraria ;
                                rdfs:range xsd:nonNegativeInteger ;
                                rdfs:comment "Una risorsa letteraria ha un determinato numero di pagine"@it ;
                                rdfs:label "ha_numpag"@it .


###  http://www.semanticweb.org/produzione_letteraria#ha_periodicità
produzione_letteraria:ha_periodicità rdf:type owl:DatatypeProperty ;
                                     rdfs:domain produzione_letteraria:Rivista ;
                                     rdfs:range [ rdf:type rdfs:Datatype ;
                                                  owl:oneOf [ rdf:type rdf:List ;
                                                              rdf:first "annuale" ;
                                                              rdf:rest [ rdf:type rdf:List ;
                                                                         rdf:first "mensile" ;
                                                                         rdf:rest [ rdf:type rdf:List ;
                                                                                    rdf:first "settimanale" ;
                                                                                    rdf:rest rdf:nil
                                                                                  ]
                                                                       ]
                                                            ]
                                                ] ;
                                     rdfs:comment "Una rivista può essere rilasciata con una periodicità settimanale, mensile o annuale"@it ;
                                     rdfs:label "ha_periodicità"@it .


###  http://www.semanticweb.org/produzione_letteraria#ha_titolo
produzione_letteraria:ha_titolo rdf:type owl:DatatypeProperty ,
                                         owl:FunctionalProperty ;
                                rdfs:domain produzione_letteraria:Risorsa_letteraria ;
                                rdfs:range xsd:string ;
                                rdfs:comment "Una risorsa letteraria ha un titolo"@it ;
                                rdfs:label "ha_titolo"@it .


###  http://www.semanticweb.org/produzione_letteraria#ha_trama
produzione_letteraria:ha_trama rdf:type owl:DatatypeProperty ,
                                        owl:FunctionalProperty ;
                               rdfs:domain produzione_letteraria:Risorsa_letteraria ;
                               rdfs:range xsd:string ;
                               rdfs:comment "Una risorsa letteraria ha una trama che serve per riassumere la lettura"@it ;
                               rdfs:label "ha_trama"@it .


###  http://www.semanticweb.org/produzione_letteraria#tipo_versi
produzione_letteraria:tipo_versi rdf:type owl:DatatypeProperty ,
                                          owl:FunctionalProperty ;
                                 rdfs:domain produzione_letteraria:Poesia ;
                                 rdfs:range [ rdf:type rdfs:Datatype ;
                                              owl:oneOf [ rdf:type rdf:List ;
                                                          rdf:first "alternata" ;
                                                          rdf:rest [ rdf:type rdf:List ;
                                                                     rdf:first "baciata" ;
                                                                     rdf:rest [ rdf:type rdf:List ;
                                                                                rdf:first "incatenata" ;
                                                                                rdf:rest [ rdf:type rdf:List ;
                                                                                           rdf:first "incrociata" ;
                                                                                           rdf:rest [ rdf:type rdf:List ;
                                                                                                      rdf:first "sciolta" ;
                                                                                                      rdf:rest rdf:nil
                                                                                                    ]
                                                                                         ]
                                                                              ]
                                                                   ]
                                                        ]
                                            ] ;
                                 rdfs:comment "Una poesia è presentata attraverso rime di diverso tipo"@it ;
                                 rdfs:label "tipo_versi"@it .


###  http://www.semanticweb.org/produzione_letteraria#èIn_lingua
produzione_letteraria:èIn_lingua rdf:type owl:DatatypeProperty ;
                                 rdfs:domain produzione_letteraria:Risorsa_letteraria ;
                                 rdfs:range xsd:language ;
                                 rdfs:comment "Una risorsa letteraria è stata scritta in una certa lingua"@it ;
                                 rdfs:label "èIn_lingua"@it .


###  http://www.semanticweb.org/produzione_letteraria#èRaccoltaOrdinata
produzione_letteraria:èRaccoltaOrdinata rdf:type owl:DatatypeProperty ,
                                                 owl:FunctionalProperty ;
                                        rdfs:domain produzione_letteraria:Risorsa_letteraria ;
                                        rdfs:range xsd:boolean ;
                                        rdfs:comment "Una risorsa letteraria può essere una raccolta di elementi che segue un ordine"@it ;
                                        rdfs:label "èRaccoltaOrdinata"@it .


###  http://www.semanticweb.org/produzione_letteraria#èuscitoIn
