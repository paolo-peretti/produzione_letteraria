    <!-- http://www.semanticweb.org/produzione_letteraria#codice_lettura -->

    <owl:DatatypeProperty rdf:about="http://www.semanticweb.org/produzione_letteraria#codice_lettura">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/produzione_letteraria#Fumetto"/>
        <rdfs:range>
            <rdfs:Datatype>
                <owl:oneOf>
                    <rdf:Description>
                        <rdf:type rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#List"/>
