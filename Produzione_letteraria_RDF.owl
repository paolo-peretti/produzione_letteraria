    <!-- http://www.semanticweb.org/produzione_letteraria#Autore -->

    <owl:Class rdf:about="http://www.semanticweb.org/produzione_letteraria#Autore">
        <owl:equivalentClass>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.semanticweb.org/produzione_letteraria#ha_scritto"/>
                <owl:someValuesFrom rdf:resource="http://www.semanticweb.org/produzione_letteraria#Risorsa_letteraria"/>
            </owl:Restriction>
        </owl:equivalentClass>
        <rdfs:subClassOf rdf:resource="http://xmlns.com/foaf/0.1/Person"/>
        <rdfs:comment xml:lang="it">Persona che ha prodotto almeno 1 risorsa letteraria</rdfs:comment>
        <rdfs:label xml:lang="it">Autore</rdfs:label>
        <skos:broadMatch rdf:resource="http://purl.org/dc/elements/1.1/creator"/>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/produzione_letteraria#Collezione -->

    <owl:Class rdf:about="http://www.semanticweb.org/produzione_letteraria#Collezione">
        <owl:equivalentClass>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.semanticweb.org/produzione_letteraria#contiene"/>
                <owl:minQualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">2</owl:minQualifiedCardinality>
                <owl:onClass rdf:resource="http://www.semanticweb.org/produzione_letteraria#Risorsa_letteraria"/>
            </owl:Restriction>
        </owl:equivalentClass>
        <rdfs:comment xml:lang="it">Una collezione è un insieme di risorse letterarie.</rdfs:comment>
        <rdfs:label xml:lang="it">Collezione</rdfs:label>
        <skos:broadMatch rdf:resource="http://purl.org/ontology/bibo/CollectedDocument"/>
        <skos:broadMatch>http://purl.org/dc/dcmitype/Collection</skos:broadMatch>
        <skos:broadMatch>http://purl.org/ontology/bibo/Collection</skos:broadMatch>
        <skos:broadMatch>http://purl.org/ontology/bibo/Series</skos:broadMatch>
        <skos:broadMatch>http://purl.org/spar/fabio/Catalog</skos:broadMatch>
        <skos:narrowMatch>http://purl.org/ontology/bibo/Book</skos:narrowMatch>
        <skos:narrowMatch>http://purl.org/ontology/bibo/Document</skos:narrowMatch>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/produzione_letteraria#Dizionario -->

    <owl:Class rdf:about="http://www.semanticweb.org/produzione_letteraria#Dizionario">
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://www.semanticweb.org/produzione_letteraria#descrive_lingua"/>
                        <owl:someValuesFrom rdf:resource="http://www.w3.org/2001/XMLSchema#language"/>
                    </owl:Restriction>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://www.semanticweb.org/produzione_letteraria#èRaccoltaOrdinata"/>
                        <owl:someValuesFrom>
                            <rdfs:Datatype>
                                <owl:oneOf>
                                    <rdf:Description>
                                        <rdf:type rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#List"/>
                                        <rdf:first rdf:datatype="http://www.w3.org/2001/XMLSchema#boolean">true</rdf:first>
                                        <rdf:rest rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#nil"/>
                                    </rdf:Description>
                                </owl:oneOf>
                            </rdfs:Datatype>
                        </owl:someValuesFrom>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </owl:equivalentClass>
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/produzione_letteraria#Libro_Classico"/>
        <rdfs:comment xml:lang="it">Il dizionario è una raccolta ordinata di descrizioni di termini, questa risorsa permette di descrivere una lingua</rdfs:comment>
        <rdfs:label xml:lang="it">Dizionario</rdfs:label>
        <skos:exactMatch>http://purl.org/spar/fabio/TermDictionary</skos:exactMatch>
        <skos:exactMatch>http://purl.org/spar/fabio/Vocabulary</skos:exactMatch>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/produzione_letteraria#Enciclopedia -->

    <owl:Class rdf:about="http://www.semanticweb.org/produzione_letteraria#Enciclopedia">
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <owl:Class>
                        <owl:unionOf rdf:parseType="Collection">
                            <owl:Class>
                                <owl:oneOf rdf:parseType="Collection">
                                    <rdf:Description rdf:about="http://www.semanticweb.org/produzione_letteraria#scienza"/>
                                </owl:oneOf>
                            </owl:Class>
                            <owl:Class>
                                <owl:oneOf rdf:parseType="Collection">
                                    <rdf:Description rdf:about="http://www.semanticweb.org/produzione_letteraria#storia"/>
                                </owl:oneOf>
                            </owl:Class>
                            <owl:Restriction>
                                <owl:onProperty rdf:resource="http://www.semanticweb.org/produzione_letteraria#trattaDi"/>
                                <owl:someValuesFrom>
                                    <owl:Class>
                                        <owl:oneOf rdf:parseType="Collection">
                                            <rdf:Description rdf:about="http://www.semanticweb.org/produzione_letteraria#arte"/>
                                        </owl:oneOf>
                                    </owl:Class>
                                </owl:someValuesFrom>
                            </owl:Restriction>
                        </owl:unionOf>
                    </owl:Class>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://www.semanticweb.org/produzione_letteraria#èRaccoltaOrdinata"/>
                        <owl:hasValue rdf:datatype="http://www.w3.org/2001/XMLSchema#boolean">true</owl:hasValue>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </owl:equivalentClass>
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/produzione_letteraria#Libro_Classico"/>
        <rdfs:comment xml:lang="it">L&apos;enciclopedia è una raccolta ordinata di spiegazioni di carattere artistico, storico o scientifico</rdfs:comment>
        <rdfs:label xml:lang="it">Enciclopedia</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/produzione_letteraria#Fumettista -->

    <owl:Class rdf:about="http://www.semanticweb.org/produzione_letteraria#Fumettista">
        <owl:equivalentClass>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.semanticweb.org/produzione_letteraria#ha_scritto"/>
                <owl:someValuesFrom rdf:resource="http://www.semanticweb.org/produzione_letteraria#Fumetto"/>
            </owl:Restriction>
        </owl:equivalentClass>
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/produzione_letteraria#Autore"/>
        <rdfs:comment xml:lang="it">Il fumettista è un autore che ha scritto qualche fumetto</rdfs:comment>
        <rdfs:label xml:lang="it">Fumettista</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/produzione_letteraria#Fumetto -->

    <owl:Class rdf:about="http://www.semanticweb.org/produzione_letteraria#Fumetto">
        <owl:equivalentClass>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.semanticweb.org/produzione_letteraria#compostoDa_vignette"/>
                <owl:hasValue rdf:datatype="http://www.w3.org/2001/XMLSchema#boolean">true</owl:hasValue>
            </owl:Restriction>
        </owl:equivalentClass>
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/produzione_letteraria#Risorsa_letteraria"/>
        <owl:disjointUnionOf rdf:parseType="Collection">
            <rdf:Description rdf:about="http://www.semanticweb.org/produzione_letteraria#Fumetto_Occidentale"/>
            <rdf:Description rdf:about="http://www.semanticweb.org/produzione_letteraria#Manga"/>
        </owl:disjointUnionOf>
        <rdfs:comment xml:lang="it">Il fumetto è una opera letteraria che si differenzia per il fatto che è composta da vignette, una vignetta è composta da immagini e dialoghi</rdfs:comment>
        <rdfs:label xml:lang="it">Fumetto</rdfs:label>
        <skos:narrowMatch>http://purl.org/eprint/type/BookItem</skos:narrowMatch>
        <skos:narrowMatch>http://purl.org/ontology/bibo/Chapter</skos:narrowMatch>
